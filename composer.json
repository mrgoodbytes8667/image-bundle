{
    "name": "mrgoodbytes8667/image-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for image caching",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "doctrine/orm": "^2.8",
        "imagine/imagine": "^1.3.3",
        "mrgoodbytes8667/enum-serializer-bundle": "^3.0 | ^4.0",
        "mrgoodbytes8667/response-bundle": "^4.0 | ^5.0 | ^6.0",
        "symfony/polyfill-uuid": "^1.20",
        "symfony/validator": "^6.0"
    },
    "require-dev": {
        "doctrine/common": "^3.0",
        "mrgoodbytes8667/test-common": ">=0.0.27",
        "mrgoodbytes8667/test-common-faker": "^0.3.4",
        "rector/rector": "^0.13 | ^0.14 | ^0.15",
        "symfony/test-pack": "^1.0.8",
        "symfony/var-dumper": "^6.0"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@mrgoodbytes.dev"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "conflict": {
        "doctrine/annotations": "<1.11.1",
        "doctrine/cache": "<1.10.2",
        "doctrine/collections": "<1.6.7",
        "doctrine/event-manager": "<1.1.1",
        "doctrine/inflector": "<1.4.3",
        "doctrine/lexer": "<1.2.1",
        "doctrine/persistence": "<2.1.0",
        "phpunit/phpunit": "<10.0",
        "symfony/framework-bundle": "<5.3.0",
        "symfony/property-access": "<5.3.0",
        "symfony/property-info": "<5.3.0",
        "symfony/serializer": "<5.2.2"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\ImageBundle\\": "src/",
            "Bytes\\ImageBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
